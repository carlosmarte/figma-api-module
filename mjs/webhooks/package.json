{
  "name": "figma-webhooks",
  "version": "1.0.0",
  "description": "Node.js client for Figma Webhooks API v2",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "figma-webhooks": "cli.mjs"
  },
  "scripts": {
    "build": "echo 'No build step required for ES modules'",
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "./cli.mjs --help"
  },
  "keywords": [
    "figma",
    "webhooks",
    "api",
    "client",
    "sdk",
    "design",
    "automation",
    "workflow"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "*.mjs",
    "*.d.mts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "peerDependencies": {
    "@figma-api/fetch": "file:../figma-fetch"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@figma-api/fetch": "file:../figma-fetch",
    "@jest/globals": "^29.7.0",
    "eslint": "^8.57.0",
    "fastify": "^5.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkeloquent/figma-api-module.git",
    "directory": "mjs/webhooks"
  },
  "bugs": {
    "url": "https://github.com/thinkeloquent/figma-api-module/issues"
  },
  "homepage": "https://github.com/thinkeloquent/figma-api-module/tree/main/mjs/webhooks#readme"
}
