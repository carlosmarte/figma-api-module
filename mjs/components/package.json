{
  "name": "figma-components-api",
  "version": "1.0.0",
  "description": "Node.js client for Figma Components, Component Sets, and Styles API",
  "type": "module",
  "main": "src/interfaces/sdk.mjs",
  "exports": {
    ".": "./index.mjs",
    "./client": "./src/core/client.mjs",
    "./service": "./src/core/service.mjs",
    "./exceptions": "./src/core/exceptions.mjs",
    "./sdk": "./src/interfaces/sdk.mjs",
    "./cli": "./src/interfaces/cli.mjs"
  },
  "bin": {
    "figma-components": "./src/interfaces/cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "npm run lint && npm test",
    "cli:help": "node src/interfaces/cli.mjs --help",
    "example:team-components": "node src/interfaces/cli.mjs components list-team YOUR_TEAM_ID",
    "example:file-components": "node src/interfaces/cli.mjs components list-file YOUR_FILE_KEY",
    "example:get-component": "node src/interfaces/cli.mjs components get YOUR_COMPONENT_KEY",
    "example:team-library": "node src/interfaces/cli.mjs library get-team YOUR_TEAM_ID",
    "example:search": "node src/interfaces/cli.mjs components search YOUR_TEAM_ID button"
  },
  "keywords": [
    "figma", 
    "api", 
    "client", 
    "sdk", 
    "components", 
    "component-sets", 
    "styles", 
    "design-system", 
    "library"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "jest": {
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".mjs"],
    "transform": {},
    "testMatch": ["**/tests/**/*.test.mjs"],
    "collectCoverageFrom": [
      "src/**/*.mjs",
      "!src/interfaces/cli.mjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figma/rest-api-spec.git"
  },
  "bugs": {
    "url": "https://github.com/figma/rest-api-spec/issues"
  },
  "homepage": "https://www.figma.com/developers/api#library-items"
}