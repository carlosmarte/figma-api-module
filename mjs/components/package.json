{
  "name": "figma-components-api",
  "version": "1.0.0",
  "description": "Node.js client for Figma Components, Component Sets, and Styles API",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./service": {
      "types": "./src/core/service.d.mts",
      "import": "./src/core/service.mjs"
    },
    "./exceptions": {
      "types": "./src/core/exceptions.d.mts",
      "import": "./src/core/exceptions.mjs"
    },
    "./sdk": {
      "types": "./src/interfaces/sdk.d.mts",
      "import": "./src/interfaces/sdk.mjs"
    },
    "./cli": {
      "types": "./src/interfaces/cli.d.mts",
      "import": "./src/interfaces/cli.mjs"
    }
  },
  "bin": {
    "figma-components": "src/interfaces/cli.mjs"
  },
  "scripts": {
    "build": "echo 'No build step required for ES modules'",
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "npm run lint && npm test",
    "cli:help": "node src/interfaces/cli.mjs --help",
    "example:team-components": "node src/interfaces/cli.mjs components list-team YOUR_TEAM_ID",
    "example:file-components": "node src/interfaces/cli.mjs components list-file YOUR_FILE_KEY",
    "example:get-component": "node src/interfaces/cli.mjs components get YOUR_COMPONENT_KEY",
    "example:team-library": "node src/interfaces/cli.mjs library get-team YOUR_TEAM_ID",
    "example:search": "node src/interfaces/cli.mjs components search YOUR_TEAM_ID button"
  },
  "keywords": [
    "figma",
    "api",
    "client",
    "sdk",
    "components",
    "component-sets",
    "styles",
    "design-system",
    "library"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "src",
    "index.mjs",
    "index.d.mts",
    "README.md",
    "EXAMPLES.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "peerDependencies": {
    "@figma-api/fetch": "file:../figma-fetch"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@figma-api/fetch": "file:../figma-fetch",
    "@jest/globals": "^29.7.0",
    "eslint": "^8.57.0",
    "fastify": "^5.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figma/figma-api-module.git",
    "directory": "mjs/components"
  },
  "bugs": {
    "url": "https://github.com/figma/figma-api-module/issues"
  },
  "homepage": "https://github.com/figma/figma-api-module/tree/main/mjs/components#readme"
}
