{
  "name": "figma-files-api",
  "version": "1.0.0",
  "description": "Node.js client for Figma Files API",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "figma-files": "src/interfaces/cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "cli:help": "node src/interfaces/cli.mjs --help",
    "example:get-file": "node src/interfaces/cli.mjs get --file-key YOUR_FILE_KEY",
    "example:render-images": "node src/interfaces/cli.mjs render-images --file-key YOUR_FILE_KEY --ids NODE_ID"
  },
  "keywords": [
    "figma",
    "api",
    "client",
    "sdk",
    "files",
    "design"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "src",
    "index.mjs",
    "index.d.mts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figma/figma-api-module.git",
    "directory": "mjs/files"
  },
  "bugs": {
    "url": "https://github.com/figma/figma-api-module/issues"
  },
  "homepage": "https://github.com/figma/figma-api-module/tree/main/mjs/files#readme"
}
