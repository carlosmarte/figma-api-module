{
  "name": "figma-variables-sdk",
  "version": "1.0.0",
  "description": "Node.js client for Figma Variables API",
  "type": "module",
  "main": "index.mjs",
  "exports": {
    ".": "./index.mjs",
    "./client": "./src/core/client.mjs",
    "./service": "./src/core/service.mjs",
    "./exceptions": "./src/core/exceptions.mjs",
    "./sdk": "./src/interfaces/sdk.mjs",
    "./cli": "./src/interfaces/cli.mjs"
  },
  "bin": {
    "figma-variables": "./src/interfaces/cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "npm run lint && npm test"
  },
  "keywords": [
    "figma",
    "api",
    "variables",
    "client",
    "sdk",
    "enterprise"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "src",
    "index.mjs",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  }
}
