{
  "name": "figma-library-analytics",
  "version": "1.0.0",
  "description": "Node.js client for Figma Library Analytics API",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "figma-library-analytics": "cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:integration": "node integration-test.mjs",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "./cli.mjs --help"
  },
  "keywords": [
    "figma",
    "library",
    "analytics",
    "api",
    "client",
    "sdk",
    "design",
    "system",
    "components",
    "styles",
    "variables",
    "usage",
    "metrics"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "*.mjs",
    "*.d.mts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkeloquent/figma-api-module.git",
    "directory": "mjs/library-analytics"
  },
  "bugs": {
    "url": "https://github.com/thinkeloquent/figma-api-module/issues"
  },
  "homepage": "https://github.com/thinkeloquent/figma-api-module/tree/main/mjs/library-analytics#readme"
}
