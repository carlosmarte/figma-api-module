{
  "name": "figma-projects",
  "version": "1.0.0",
  "description": "Node.js client for Figma Projects API with CLI interface",
  "type": "module",
  "main": "index.mjs",
  "bin": {
    "figma-projects": "./src/interfaces/cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "figma",
    "api",
    "projects",
    "client",
    "sdk",
    "cli",
    "design",
    "automation"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "testMatch": [
      "**/tests/**/*.test.mjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkeloquent/figma-projects"
  },
  "bugs": {
    "url": "https://github.com/thinkeloquent/figma-projects/issues"
  },
  "homepage": "https://github.com/thinkeloquent/figma-projects#readme"
}
