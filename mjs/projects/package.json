{
  "name": "figma-projects",
  "version": "1.0.0",
  "description": "Node.js client for Figma Projects API with CLI interface",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.mts",
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "import": "./index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "figma-projects": "src/interfaces/cli.mjs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ../node_modules/.bin/jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "figma",
    "api",
    "projects",
    "client",
    "sdk",
    "cli",
    "design",
    "automation"
  ],
  "author": "thinkeloquent",
  "license": "MIT",
  "files": [
    "src",
    "index.mjs",
    "index.d.mts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkeloquent/figma-api-module.git",
    "directory": "mjs/projects"
  },
  "bugs": {
    "url": "https://github.com/thinkeloquent/figma-api-module/issues"
  },
  "homepage": "https://github.com/thinkeloquent/figma-api-module/tree/main/mjs/projects#readme"
}
